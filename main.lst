   55                        	;
   56                        	; init_random - Initialize a 32-bit LFSR using an 8-bit seed value in Y.
   57                        	;
   58                        	; The LFSR is initialized to n'th entry of a standard CRC-32 lookup-table,
   59                        	; which gives it a decent distribution of bits.
   60                        	;
   61                        	; Since seed is an 8-bit value, there are 255 (256-1) possible starting
   62                        	; states for the LFSR, because 0 would generate a 0 state.
   63                        	;
   64                        	; CRC-32 code by Paul Guertin. See http://6502.org/source/integers/crc.htm
   65                        	;
   66                        	
   67    02:CB0C             	init_random	.proc
   68                        	
   69    02:CB0C  A9 01      			lda	#1			; Init CRC-32 table value.
   70    02:CB0E  85 98      			sta	<random + 0
   71    02:CB10  98         			tya				; Get and check the seed value.
   72    02:CB11  D0 01      			bne	.reverse_seed
   73    02:CB13  3A         			dec	a			; Which must be non-zero!
   74    02:CB14  4A         	.reverse_seed:	lsr	a			; Reverse the bits so that small changes
   75    02:CB15  26 98      			rol	<random + 0		; in the seed make larger differences in
   76    02:CB17  90 FB      			bcc	.reverse_seed		; the initial state.
   77                        	
   78    02:CB19  64 99      			stz	<random + 1		; A contains the high byte of the CRC-32.
   79    02:CB1B  64 9A      			stz	<random + 2		; The other three bytes are in memory.
   80    02:CB1D  62         			cla
   81                        	
   82    02:CB1E  A0 08      			ldy	#8			; Y counts bits in a byte.
   83    02:CB20  4A         	.bit_loop:	lsr	a			; The CRC-32 algorithm is similar to CRC-16
   84    02:CB21  66 9A      			ror	<random + 2		; except that it is reversed (originally for
   85    02:CB23  66 99      			ror	<random + 1		; hardware reasons). This is why we shift
   86    02:CB25  66 98      			ror	<random + 0		; right instead of left here.
   87    02:CB27  90 16      			bcc	.no_add			; Do nothing if no overflow,
   88    02:CB29  49 ED      			eor	#$ED			; else add CRC-32 polynomial $EDB88320.
   89                        	
   90    02:CB2B  48         			pha				; Save high byte while we do others.
   91    02:CB2C  A5 9A      			lda	<random + 2
   92    02:CB2E  49 B8      			eor	#$B8			; Most reference books give the CRC-32 poly
   93    02:CB30  85 9A      			sta	<random + 2		; as $04C11DB7. This is actually the same if
   94    02:CB32  A5 99      			lda	<random + 1		; you write it in binary and read it right-
   95    02:CB34  49 83      			eor	#$83			; to-left instead of left-to-right. Doing it
   96    02:CB36  85 99      			sta	<random + 1		; this way means we won't have to explicitly
   97    02:CB38  A5 98      			lda	<random + 0		; reverse things afterwards.
   98    02:CB3A  49 20      			eor	#$20
   99    02:CB3C  85 98      			sta	<random + 0
  100    02:CB3E  68         			pla				; Restore high byte.
  101                        	
  102    02:CB3F  88         	.no_add:	dey				; Do next bit.
  103    02:CB40  D0 DE      			bne	.bit_loop
  104                        	
  105    02:CB42  85 9B      			sta	<random + 3		; Save CRC-32 high-byte.
  106                        	
  107                        			leave				; All done!
         02:CB44  4C EF FF   			jmp	leave_proc
  108                        	
  109                        			.endp
