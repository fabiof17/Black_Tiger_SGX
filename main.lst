   55                        	;
   56                        	; init_random - Initialize a 32-bit LFSR using an 8-bit seed value in Y.
   57                        	;
   58                        	; The LFSR is initialized to n'th entry of a standard CRC-32 lookup-table,
   59                        	; which gives it a decent distribution of bits.
   60                        	;
   61                        	; Since seed is an 8-bit value, there are 255 (256-1) possible starting
   62                        	; states for the LFSR, because 0 would generate a 0 state.
   63                        	;
   64                        	; CRC-32 code by Paul Guertin. See http://6502.org/source/integers/crc.htm
   65                        	;
   66                        	
   67    11:D0AE             	init_random	.proc
   68                        	
   69    11:D0AE  A9 01      			lda	#1			; Init CRC-32 table value.
   70    11:D0B0  85 98      			sta	<random + 0
   71    11:D0B2  98         			tya				; Get and check the seed value.
   72    11:D0B3  D0 01      			bne	.reverse_seed
   73    11:D0B5  3A         			dec	a			; Which must be non-zero!
   74    11:D0B6  4A         	.reverse_seed:	lsr	a			; Reverse the bits so that small changes
   75    11:D0B7  26 98      			rol	<random + 0		; in the seed make larger differences in
   76    11:D0B9  90 FB      			bcc	.reverse_seed		; the initial state.
   77                        	
   78    11:D0BB  64 99      			stz	<random + 1		; A contains the high byte of the CRC-32.
   79    11:D0BD  64 9A      			stz	<random + 2		; The other three bytes are in memory.
   80    11:D0BF  62         			cla
   81                        	
   82    11:D0C0  A0 08      			ldy	#8			; Y counts bits in a byte.
   83    11:D0C2  4A         	.bit_loop:	lsr	a			; The CRC-32 algorithm is similar to CRC-16
   84    11:D0C3  66 9A      			ror	<random + 2		; except that it is reversed (originally for
   85    11:D0C5  66 99      			ror	<random + 1		; hardware reasons). This is why we shift
   86    11:D0C7  66 98      			ror	<random + 0		; right instead of left here.
   87    11:D0C9  90 16      			bcc	.no_add			; Do nothing if no overflow,
   88    11:D0CB  49 ED      			eor	#$ED			; else add CRC-32 polynomial $EDB88320.
   89                        	
   90    11:D0CD  48         			pha				; Save high byte while we do others.
   91    11:D0CE  A5 9A      			lda	<random + 2
   92    11:D0D0  49 B8      			eor	#$B8			; Most reference books give the CRC-32 poly
   93    11:D0D2  85 9A      			sta	<random + 2		; as $04C11DB7. This is actually the same if
   94    11:D0D4  A5 99      			lda	<random + 1		; you write it in binary and read it right-
   95    11:D0D6  49 83      			eor	#$83			; to-left instead of left-to-right. Doing it
   96    11:D0D8  85 99      			sta	<random + 1		; this way means we won't have to explicitly
   97    11:D0DA  A5 98      			lda	<random + 0		; reverse things afterwards.
   98    11:D0DC  49 20      			eor	#$20
   99    11:D0DE  85 98      			sta	<random + 0
  100    11:D0E0  68         			pla				; Restore high byte.
  101                        	
  102    11:D0E1  88         	.no_add:	dey				; Do next bit.
  103    11:D0E2  D0 DE      			bne	.bit_loop
  104                        	
  105    11:D0E4  85 9B      			sta	<random + 3		; Save CRC-32 high-byte.
  106                        	
  107                        			leave				; All done!
         11:D0E6  4C EF FF   			jmp	leave_proc
  108                        	
  109                        			.endp
