   55                        	;
   56                        	; init_random - Initialize a 32-bit LFSR using an 8-bit seed value in Y.
   57                        	;
   58                        	; The LFSR is initialized to n'th entry of a standard CRC-32 lookup-table,
   59                        	; which gives it a decent distribution of bits.
   60                        	;
   61                        	; Since seed is an 8-bit value, there are 255 (256-1) possible starting
   62                        	; states for the LFSR, because 0 would generate a 0 state.
   63                        	;
   64                        	; CRC-32 code by Paul Guertin. See http://6502.org/source/integers/crc.htm
   65                        	;
   66                        	
   67    18:D21D             	init_random	.proc
   68                        	
   69    18:D21D  A9 01      			lda	#1			; Init CRC-32 table value.
   70    18:D21F  85 98      			sta	<random + 0
   71    18:D221  98         			tya				; Get and check the seed value.
   72    18:D222  D0 01      			bne	.reverse_seed
   73    18:D224  3A         			dec	a			; Which must be non-zero!
   74    18:D225  4A         	.reverse_seed:	lsr	a			; Reverse the bits so that small changes
   75    18:D226  26 98      			rol	<random + 0		; in the seed make larger differences in
   76    18:D228  90 FB      			bcc	.reverse_seed		; the initial state.
   77                        	
   78    18:D22A  64 99      			stz	<random + 1		; A contains the high byte of the CRC-32.
   79    18:D22C  64 9A      			stz	<random + 2		; The other three bytes are in memory.
   80    18:D22E  62         			cla
   81                        	
   82    18:D22F  A0 08      			ldy	#8			; Y counts bits in a byte.
   83    18:D231  4A         	.bit_loop:	lsr	a			; The CRC-32 algorithm is similar to CRC-16
   84    18:D232  66 9A      			ror	<random + 2		; except that it is reversed (originally for
   85    18:D234  66 99      			ror	<random + 1		; hardware reasons). This is why we shift
   86    18:D236  66 98      			ror	<random + 0		; right instead of left here.
   87    18:D238  90 16      			bcc	.no_add			; Do nothing if no overflow,
   88    18:D23A  49 ED      			eor	#$ED			; else add CRC-32 polynomial $EDB88320.
   89                        	
   90    18:D23C  48         			pha				; Save high byte while we do others.
   91    18:D23D  A5 9A      			lda	<random + 2
   92    18:D23F  49 B8      			eor	#$B8			; Most reference books give the CRC-32 poly
   93    18:D241  85 9A      			sta	<random + 2		; as $04C11DB7. This is actually the same if
   94    18:D243  A5 99      			lda	<random + 1		; you write it in binary and read it right-
   95    18:D245  49 83      			eor	#$83			; to-left instead of left-to-right. Doing it
   96    18:D247  85 99      			sta	<random + 1		; this way means we won't have to explicitly
   97    18:D249  A5 98      			lda	<random + 0		; reverse things afterwards.
   98    18:D24B  49 20      			eor	#$20
   99    18:D24D  85 98      			sta	<random + 0
  100    18:D24F  68         			pla				; Restore high byte.
  101                        	
  102    18:D250  88         	.no_add:	dey				; Do next bit.
  103    18:D251  D0 DE      			bne	.bit_loop
  104                        	
  105    18:D253  85 9B      			sta	<random + 3		; Save CRC-32 high-byte.
  106                        	
  107                        			leave				; All done!
