   55                        	;
   56                        	; init_random - Initialize a 32-bit LFSR using an 8-bit seed value in Y.
   57                        	;
   58                        	; The LFSR is initialized to n'th entry of a standard CRC-32 lookup-table,
   59                        	; which gives it a decent distribution of bits.
   60                        	;
   61                        	; Since seed is an 8-bit value, there are 255 (256-1) possible starting
   62                        	; states for the LFSR, because 0 would generate a 0 state.
   63                        	;
   64                        	; CRC-32 code by Paul Guertin. See http://6502.org/source/integers/crc.htm
   65                        	;
   66                        	
   67    01:DC03             	init_random	.proc
   68                        	
   69    01:DC03  A9 01      			lda	#1			; Init CRC-32 table value.
   70    01:DC05  85 98      			sta	<random + 0
   71    01:DC07  98         			tya				; Get and check the seed value.
   72    01:DC08  D0 01      			bne	.reverse_seed
   73    01:DC0A  3A         			dec	a			; Which must be non-zero!
   74    01:DC0B  4A         	.reverse_seed:	lsr	a			; Reverse the bits so that small changes
   75    01:DC0C  26 98      			rol	<random + 0		; in the seed make larger differences in
   76    01:DC0E  90 FB      			bcc	.reverse_seed		; the initial state.
   77                        	
   78    01:DC10  64 99      			stz	<random + 1		; A contains the high byte of the CRC-32.
   79    01:DC12  64 9A      			stz	<random + 2		; The other three bytes are in memory.
   80    01:DC14  62         			cla
   81                        	
   82    01:DC15  A0 08      			ldy	#8			; Y counts bits in a byte.
   83    01:DC17  4A         	.bit_loop:	lsr	a			; The CRC-32 algorithm is similar to CRC-16
   84    01:DC18  66 9A      			ror	<random + 2		; except that it is reversed (originally for
   85    01:DC1A  66 99      			ror	<random + 1		; hardware reasons). This is why we shift
   86    01:DC1C  66 98      			ror	<random + 0		; right instead of left here.
   87    01:DC1E  90 16      			bcc	.no_add			; Do nothing if no overflow,
   88    01:DC20  49 ED      			eor	#$ED			; else add CRC-32 polynomial $EDB88320.
   89                        	
   90    01:DC22  48         			pha				; Save high byte while we do others.
   91    01:DC23  A5 9A      			lda	<random + 2
   92    01:DC25  49 B8      			eor	#$B8			; Most reference books give the CRC-32 poly
   93    01:DC27  85 9A      			sta	<random + 2		; as $04C11DB7. This is actually the same if
   94    01:DC29  A5 99      			lda	<random + 1		; you write it in binary and read it right-
   95    01:DC2B  49 83      			eor	#$83			; to-left instead of left-to-right. Doing it
   96    01:DC2D  85 99      			sta	<random + 1		; this way means we won't have to explicitly
   97    01:DC2F  A5 98      			lda	<random + 0		; reverse things afterwards.
   98    01:DC31  49 20      			eor	#$20
   99    01:DC33  85 98      			sta	<random + 0
  100    01:DC35  68         			pla				; Restore high byte.
  101                        	
  102    01:DC36  88         	.no_add:	dey				; Do next bit.
  103    01:DC37  D0 DE      			bne	.bit_loop
  104                        	
  105    01:DC39  85 9B      			sta	<random + 3		; Save CRC-32 high-byte.
  106                        	
  107                        			leave				; All done!
         01:DC3B  4C EF FF   			jmp	leave_proc
  108                        	
  109                        			.endp
