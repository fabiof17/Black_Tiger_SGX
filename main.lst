   55                        	;
   56                        	; init_random - Initialize a 32-bit LFSR using an 8-bit seed value in Y.
   57                        	;
   58                        	; The LFSR is initialized to n'th entry of a standard CRC-32 lookup-table,
   59                        	; which gives it a decent distribution of bits.
   60                        	;
   61                        	; Since seed is an 8-bit value, there are 255 (256-1) possible starting
   62                        	; states for the LFSR, because 0 would generate a 0 state.
   63                        	;
   64                        	; CRC-32 code by Paul Guertin. See http://6502.org/source/integers/crc.htm
   65                        	;
   66                        	
   67    02:C908             	init_random	.proc
   68                        	
   69    02:C908  A9 01      			lda	#1			; Init CRC-32 table value.
   70    02:C90A  85 98      			sta	<random + 0
   71    02:C90C  98         			tya				; Get and check the seed value.
   72    02:C90D  D0 01      			bne	.reverse_seed
   73    02:C90F  3A         			dec	a			; Which must be non-zero!
   74    02:C910  4A         	.reverse_seed:	lsr	a			; Reverse the bits so that small changes
   75    02:C911  26 98      			rol	<random + 0		; in the seed make larger differences in
   76    02:C913  90 FB      			bcc	.reverse_seed		; the initial state.
   77                        	
   78    02:C915  64 99      			stz	<random + 1		; A contains the high byte of the CRC-32.
   79    02:C917  64 9A      			stz	<random + 2		; The other three bytes are in memory.
   80    02:C919  62         			cla
   81                        	
   82    02:C91A  A0 08      			ldy	#8			; Y counts bits in a byte.
   83    02:C91C  4A         	.bit_loop:	lsr	a			; The CRC-32 algorithm is similar to CRC-16
   84    02:C91D  66 9A      			ror	<random + 2		; except that it is reversed (originally for
   85    02:C91F  66 99      			ror	<random + 1		; hardware reasons). This is why we shift
   86    02:C921  66 98      			ror	<random + 0		; right instead of left here.
   87    02:C923  90 16      			bcc	.no_add			; Do nothing if no overflow,
   88    02:C925  49 ED      			eor	#$ED			; else add CRC-32 polynomial $EDB88320.
   89                        	
   90    02:C927  48         			pha				; Save high byte while we do others.
   91    02:C928  A5 9A      			lda	<random + 2
   92    02:C92A  49 B8      			eor	#$B8			; Most reference books give the CRC-32 poly
   93    02:C92C  85 9A      			sta	<random + 2		; as $04C11DB7. This is actually the same if
   94    02:C92E  A5 99      			lda	<random + 1		; you write it in binary and read it right-
   95    02:C930  49 83      			eor	#$83			; to-left instead of left-to-right. Doing it
   96    02:C932  85 99      			sta	<random + 1		; this way means we won't have to explicitly
   97    02:C934  A5 98      			lda	<random + 0		; reverse things afterwards.
   98    02:C936  49 20      			eor	#$20
   99    02:C938  85 98      			sta	<random + 0
  100    02:C93A  68         			pla				; Restore high byte.
  101                        	
  102    02:C93B  88         	.no_add:	dey				; Do next bit.
  103    02:C93C  D0 DE      			bne	.bit_loop
  104                        	
  105    02:C93E  85 9B      			sta	<random + 3		; Save CRC-32 high-byte.
  106                        	
  107                        			leave				; All done!
