   55                        	;
   56                        	; init_random - Initialize a 32-bit LFSR using an 8-bit seed value in Y.
   57                        	;
   58                        	; The LFSR is initialized to n'th entry of a standard CRC-32 lookup-table,
   59                        	; which gives it a decent distribution of bits.
   60                        	;
   61                        	; Since seed is an 8-bit value, there are 255 (256-1) possible starting
   62                        	; states for the LFSR, because 0 would generate a 0 state.
   63                        	;
   64                        	; CRC-32 code by Paul Guertin. See http://6502.org/source/integers/crc.htm
   65                        	;
   66                        	
   67    1F:CD50             	init_random	.proc
   68                        	
   69    1F:CD50  A9 01      			lda	#1			; Init CRC-32 table value.
   70    1F:CD52  85 98      			sta	<random + 0
   71    1F:CD54  98         			tya				; Get and check the seed value.
   72    1F:CD55  D0 01      			bne	.reverse_seed
   73    1F:CD57  3A         			dec	a			; Which must be non-zero!
   74    1F:CD58  4A         	.reverse_seed:	lsr	a			; Reverse the bits so that small changes
   75    1F:CD59  26 98      			rol	<random + 0		; in the seed make larger differences in
   76    1F:CD5B  90 FB      			bcc	.reverse_seed		; the initial state.
   77                        	
   78    1F:CD5D  64 99      			stz	<random + 1		; A contains the high byte of the CRC-32.
   79    1F:CD5F  64 9A      			stz	<random + 2		; The other three bytes are in memory.
   80    1F:CD61  62         			cla
   81                        	
   82    1F:CD62  A0 08      			ldy	#8			; Y counts bits in a byte.
   83    1F:CD64  4A         	.bit_loop:	lsr	a			; The CRC-32 algorithm is similar to CRC-16
   84    1F:CD65  66 9A      			ror	<random + 2		; except that it is reversed (originally for
   85    1F:CD67  66 99      			ror	<random + 1		; hardware reasons). This is why we shift
   86    1F:CD69  66 98      			ror	<random + 0		; right instead of left here.
   87    1F:CD6B  90 16      			bcc	.no_add			; Do nothing if no overflow,
   88    1F:CD6D  49 ED      			eor	#$ED			; else add CRC-32 polynomial $EDB88320.
   89                        	
   90    1F:CD6F  48         			pha				; Save high byte while we do others.
