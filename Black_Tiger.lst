  577                        	
  578    20:C3D3  20 8F E3   			jsr	sgx_di_to_mawr		; Write $0001 to SGX VRAM.
  579    20:C3D6  A0 01      			ldy	#$01
  580    20:C3D8  8C 12 02   			sty	SGX_DL
  581    20:C3DB  9C 13 02   			stz	SGX_DH
