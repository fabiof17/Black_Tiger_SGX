  575    20:C3B3  84 F0      			sty.l	<_di			; because it won't cause
  576    20:C3B5  84 F1      			sty.h	<_di			; a screen glitch.
  577                        	
  578    20:C3B7  20 8F E3   			jsr	sgx_di_to_mawr		; Write $0001 to SGX VRAM.
  579    20:C3BA  A0 01      			ldy	#$01
  580    20:C3BC  8C 12 02   			sty	SGX_DL
  581    20:C3BF  9C 13 02   			stz	SGX_DH
  582                        	
  583    20:C3C2  20 92 E3   			jsr	vdc_di_to_mawr		; Write $0000 to VDC VRAM.
  584    20:C3C5  9C 02 02   			stz	VDC_DL
  585    20:C3C8  9C 03 02   			stz	VDC_DH
  586                        	
  587    20:C3CB  20 82 E3   			jsr	sgx_di_to_marr		; Check value in SGX VRAM.
  588    20:C3CE  AC 12 02   			ldy	SGX_DL			; $01 if found, $00 if not.
  589    20:C3D1  8C E8 22   			sty	sgx_detected
  590    20:C3D4  F0 10      			beq	!+			; Skip the rest if not SGX.
  591                        	
  592    20:C3D6  20 8F E3   			jsr	sgx_di_to_mawr		; Write $0000 to SGX VRAM
  593    20:C3D9  9C 12 02   			stz	SGX_DL			; to clean VRAM contents.
