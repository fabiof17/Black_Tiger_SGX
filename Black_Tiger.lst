   72    20:C4F8  D0 01      			bne	.reverse_seed
   73    20:C4FA  3A         			dec	a			; Which must be non-zero!
   74    20:C4FB  4A         	.reverse_seed:	lsr	a			; Reverse the bits so that small changes
   75    20:C4FC  26 98      			rol	<random + 0		; in the seed make larger differences in
   76    20:C4FE  90 FB      			bcc	.reverse_seed		; the initial state.
   77                        	
   78    20:C500  64 99      			stz	<random + 1		; A contains the high byte of the CRC-32.
   79    20:C502  64 9A      			stz	<random + 2		; The other three bytes are in memory.
   80    20:C504  62         			cla
   81                        	
   82    20:C505  A0 08      			ldy	#8			; Y counts bits in a byte.
   83    20:C507  4A         	.bit_loop:	lsr	a			; The CRC-32 algorithm is similar to CRC-16
   84    20:C508  66 9A      			ror	<random + 2		; except that it is reversed (originally for
   85    20:C50A  66 99      			ror	<random + 1		; hardware reasons). This is why we shift
   86    20:C50C  66 98      			ror	<random + 0		; right instead of left here.
   87    20:C50E  90 16      			bcc	.no_add			; Do nothing if no overflow,
   88    20:C510  49 ED      			eor	#$ED			; else add CRC-32 polynomial $EDB88320.
